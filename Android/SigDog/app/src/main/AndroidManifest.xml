<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.ifms.pibic.android.sigdog" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.Ocorrencia"
            android:label="Cadastrar Ocorrência" >
        </activity>
        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.DadosAnimal"
            android:label="Cadastrar Ocorrência" >
        </activity>
        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.DadosEndereco"
            android:label="Cadastrar Ocorrência" >
        </activity>
        <activity
            android:name="br.edu.ifms.pibic.android.sigdog.Filtrar"
            android:label="Filtrar" >
        </activity>

    </application>

</manifest>
